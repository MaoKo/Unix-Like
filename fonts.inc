
macro _glyph [_translate*]
{
common local _value, _char, _string
forward
    _string = (`_translate)
    _value = 0H
    while (_string)
        _value = ((_value) shl 1H)
        _char = ((_string) and 0FFH)
        if ((_char) = "@")
            _value = ((_value) or 1H)
        else if ((_char) <> ".")
            assert (0H)
        end if
        _string = ((_string) shr 8H)
    end while
    db (_value)
}

struct _fonts _source*, _width*, _height*, _btposw*, _btposh*
    .source:    dd (_source)
    .width:     db (_width)
    .height:    db (_height)
    .btposw:    db (_btposw)
    .btposh:    db (_btposh)
ends

